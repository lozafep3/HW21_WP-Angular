{
  "name": "AngularForm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "test": "better-npm-run test",
    "lint": "",
    "start": "node server.js --config --progress",
    "analyze": "better-npm-run analyze",
    "commit": "commit-wizard"
  },
  "betterScripts": {
    "build:dev": {
      "command": "webpack-dev-server --config --progress",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack  --env=prod --config --colors --progress",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "analyze": {
      "command": "mkdirp reports && webpack --json -profile > reports/webpack-stats.json",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "private": "true",
  "devDependencies": {
